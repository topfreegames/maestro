{
   "name": "scheduler-name",
   "game": "game-name",
   "image": "nginx:latest",
   "occupiedTimeout": 180,
   "ports": [
      {
         "containerPort": 8080,
         "protocol": "TCP",
         "name": "tcp"
      }
   ],
   "limits": {
      "memory": "512Mi",
      "cpu": "100m"
   },
   "requests": {
      "memory": "128Mi",
      "cpu": "20m"
   },
   "shutdownTimeout": 10,
   "autoscaling": {
      "min": 4,
      "up": {
         "delta": 1,
         "trigger": {
            "usage": 70,
            "window": 1,
            "threshold": 80
         },
         "cooldown": 1
      },
      "down": {
         "delta": 1,
         "trigger": {
            "usage": 50,
            "window": 1,
            "threshold": 80
         },
         "cooldown": 1
      }
   }
}
